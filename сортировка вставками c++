#include <iostream>
#include <vector>
using namespace std;

// Сортировка вставками (Insertion Sort)
void insertionSort(vector<int>& arr) {
    int n = arr.size();

    // Начинаем со второго элемента
    for (int i = 1; i < n; i++) {
        int key = arr[i];   // текущий элемент для вставки
        int j = i - 1;

        // Сдвигаем элементы, которые больше key, на одну позицию вправо
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j--;
        }

        // Вставляем key на правильное место
        arr[j + 1] = key;
    }
}

// Функция вывода массива
void printArray(const vector<int>& arr) {
    for (int num : arr)
        cout << num << " ";
    cout << endl;
}

// Главная функция
int main() {
    vector<int> arr = {12, 11, 13, 5, 6};

    cout << "Исходный массив: ";
    printArray(arr);

    insertionSort(arr);

    cout << "Отсортированный массив: ";
    printArray(arr);

    return 0;
}
